
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>HumanFileFile</title>
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script src="~/jquery-1.11.1.js"></script>
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
    <link href="~/Wp2/style.css" rel="stylesheet" />
    <link href="~/Wp2/webuploader.css" rel="stylesheet" />
    <script src="~/Wp2/jquery.js"></script>
    <script src="~/Wp2/webuploader.js"></script>

    <script src="~/Scripts/jquery.signalR-2.1.2.js"></script>
    <script src="/signalR/Hubs" type="text/javascript"></script>
    <script type="text/javascript" >
        $(function () {
            $("#aa").submit(function () {
                $.connection.hub.start().done(function () {
                    $.connection.myHub1.server.chima("@(((View_UserModel)Session["user"]).U_true_name)", "人力资源档案登记复核");
                    return true;
                });
            });
            $("#wc").click(function () {
                 $.connection.hub.start().done(function () {
                     $.connection.myHub1.server.chima("@(((View_UserModel)Session["user"]).U_true_name)", "人力资源档案登记复核");
                     location.href = '/HumanFile/HumanFileRegisterList';
                });
            })
        });
    </script>
</head>
<body>
    <div>
        <form action="/HumanFile/HumanFileFileSC?gn=@Request["gn"]" id="aa" enctype="multipart/form-data" method="post">
            <table width="100%">
                <tr>
                    <td colspan="2">
                        <font color="black">您正在做的业务是：人力资源--人力资源档案管理--文件上传</font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="submit" value="上传" class="BUTTON_STYLE1">
                        <input type="button" id="wc" value="完成"   class="BUTTON_STYLE1">
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <input type="file" class="FILE_STYLE1" name="accFile" >
                    </td>
                </tr>
            </table>
        </form>
    </div>
</body>
</html>
