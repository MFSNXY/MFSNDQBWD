@model ConfigMajorModel
@{
    Layout = null;
}
@using HR.Common
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/javascript/comm/comm.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

</head>
<body>
    <div> 
        @using (Html.BeginForm("ad", "ConfigMajor", FormMethod.Post))
            {
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">
                            您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职位设置--职位添加
                        </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        @Html.CreateSubmit()
                        <input type="button" value="返回" class="BUTTON_STYLE1"
                               onclick="window.location.href = '/ConfigMajor/Index';">
                    </td>
                </tr>
            </table>

            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">

                @*<tr>
                    <td class="TD_STYLE1" width="19%">
                        职位分类编号
                    </td>
                    <td class="TD_STYLE2" width="81%">
                        <input type="text" class="INPUT_STYLE1" name="cm.majorKindId" />
                    </td>
                </tr>*@
                <tr>
                    <td class="TD_STYLE1" width="19%">
                        职位分类名称
                    </td>
                    <td class="TD_STYLE2" width="81%">
                        @Html.DropDownList("Majorkindid", ViewBag.s as IEnumerable<SelectListItem>, new { @class = "INPUT_STYLE1" })
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1" width="19%">
                        职位编号
                    </td>
                    <td class="TD_STYLE2" width="81%">
                        @Html.TextBox("Majorid", null, new { @class = "INPUT_STYLE1" })
                        @Html.ValidationMessage("Majorid")
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1" width="19%">
                        职位名称
                    </td>
                    <td class="TD_STYLE2" width="81%">
                        @Html.TextBox("Majorname", null, new { @class = "INPUT_STYLE1" })
                        @Html.ValidationMessage("Majorname")
                    </td>
                </tr>
            </table>

        }
    </div>
</body>
</html>
