@model Model.EngageResumeModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EngageInterviewDJ</title>
    <link rel="stylesheet"
          href="../../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../../css/cwcalendar.css"
          type="text/css">
    <script type="text/javascript"
            src="../../../javascript/comm/comm.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/list.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/calendar-ch.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/jquery-1.7.2.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/locate.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/select.js">

    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/time.js">
    </script>
    <script type="text/javascript">
        $(function(){
            $(".zp").hover(function(){
                $(".dt").show();
            },
            function(){
                $(".dt").hide();
            });
        });
    </script>
    <script src="~/vue-2.4.0.js"></script>
</head>
<body>
    <div id="aa">
        <form id="recruitAction!toList" name="interviewForm" action="/HR_Fist/recruit/recruitAction!toList" method="post">
            <input type="hidden" name="operate" value="doAdopt">
            <!--	<input type="hidden" name="engageInterview.checkStatus" value=""/>
            <input type="hidden" name="engageInterview.checkComment" value=""/>
              -->
            <input type="hidden" name="engageInterview.einId" value="" />
            <input type="hidden" name="engageInterview.result" value="完成" />
            <input type="hidden" name="engageInterview.resumeId" value="542" />
            <input type="hidden" name="engageResume.resId" value="542" />
            <input type="hidden" name="engageInterview.interviewStatus" value="true" />
            <table width="100%">
                <tr>
                    <td>
                        <font color="black">您正在做的业务是：人力资源--招聘管理--面试管理--面试结果登记  </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="button" value="面试登记" class="BUTTON_STYLE1"
                               onclick="search();">
                        <input type="button" value="返回" class="BUTTON_STYLE1"
                               onclick="history.back();">

                    </td>
                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td class="TD_STYLE1">
                        职位分类
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanMajorKindName" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        职位名称
                    </td>
                    <td class="TD_STYLE2" width="20%">
                        <input type="text" value="@Model.HumanMajorName" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td width="11%" class="TD_STYLE1">
                        招聘类型
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <input type="text" value="@Model.EngageType" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td rowspan="6">
                        <img class="zp" style="margin-left:100px;width:100px;height:100px;" src="@Model.HumanPicture.Substring(@Model.HumanPicture.IndexOf(@"\Image"))" />
                        <img class="dt" style="position:absolute;width:400px;height:400px;margin:-110px 0px 0px -320px;display:none;" src="@Model.HumanPicture.Substring(@Model.HumanPicture.IndexOf(@"\Image"))" />
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        姓名
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanName" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        性别
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanSex" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        EMAIL
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" value="@Model.HumanEmail" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        电话
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanTelephone" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        家庭电话
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanHomephone" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        手机
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" value="@Model.HumanMobilephone" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        住址
                    </td>
                    <td colspan="3" class="TD_STYLE2">
                        <input type="text" value="@Model" class="INPUT_STYLE2" readonly="readonly" />
                    </td>

                    <td class="TD_STYLE1">
                        邮编
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" value="@Model.HumanAddress" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        国籍
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanNationality" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        出生地
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanBirthplace" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        生日
                    </td>
                    <td width="13%" colspan="2" class="TD_STYLE2">
                        <input type="text" value="@Model.HumanBirthday.ToString().Replace("/", "-")" class="INPUT_STYLE2" readonly="readonly" />
                    </td>

                </tr>
                <tr>
                    <td width="11%" class="TD_STYLE1">
                        民族
                    </td>
                    <td class="TD_STYLE2" width="14%">
                        <input type="text" value="@Model.HumanRace" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        宗教信仰
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanReligion" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        政治面貌
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <input type="text" value="@Model.HumanParty" class="INPUT_STYLE2" readonly="readonly" />
                    </td>


                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        身份证号码
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanIdcard" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        年龄
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanAge" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        毕业院校
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanCollege" class="INPUT_STYLE2" readonly="readonly" />
                    </td>

                    <td class="TD_STYLE1">
                        学历
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanEducatedDegree" class="INPUT_STYLE2" readonly="readonly" />
                    </td>

                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        教育年限
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanEducatedYears" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        学历专业
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanEducatedMajor" class="INPUT_STYLE2" readonly="readonly" />
                    </td>

                    <td class="TD_STYLE1">
                        薪酬要求
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.DemandSalaryStandard" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        注册时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.RegistTime.ToString().Replace("/", "-")" class="INPUT_STYLE2" readonly="readonly" />
                    </td>

                </tr>
                <tr>

                    <td class="TD_STYLE1">
                        特长
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanSpecility" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        爱好
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.HumanHobby" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        推荐人
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.Checker" class="INPUT_STYLE2" readonly="readonly" />

                    </td>
                    <td class="TD_STYLE1">
                        推荐时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" value="@Model.CheckTime.ToString().Replace("/", "-")" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        个人履历
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <input type="text" value="@Model.HumanHistoryRecords" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        备注
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <input type="text" value="@Model.Remark" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        推荐意见
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <input type="text" value="@Model.Recomandation" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                </tr>


                <tr>
                    <td class="TD_STYLE1">
                        面试次数
                    </td>
                    <td class="TD_STYLE2">
                        <input type="hidden" value="1" name="engageInterview.interviewAmount" />
                        <input type="text" value="第1次面试" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        形象评价
                    </td>
                    <td class="TD_STYLE2">
                        <select name="engageInterview.imageDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        口才评价
                    </td>
                    <td class="TD_STYLE2">
                        <select name="engageInterview.nativeLanguageDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        外语口语能力
                    </td>
                    <td class="TD_STYLE1">
                        <select name="engageInterview.foreignLanguageDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        应变能力
                    </td>
                    <td class="TD_STYLE2">
                        <select name="engageInterview.responseSpeedDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        EQ
                    </td>
                    <td class="TD_STYLE2">
                        <select name="engageInterview.eqDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        IQ
                    </td>
                    <td class="TD_STYLE2">
                        <select name="engageInterview.iqDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                    <td class="TD_STYLE1">
                        综合素质
                    </td>
                    <td class="TD_STYLE1">
                        <select name="engageInterview.multiQualityDegree" class="SELECT_STYLE1">
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        面试人
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="engageInterview.register" value="zhangsan" class="INPUT_STYLE2" />
                    </td>
                    <td class="TD_STYLE1">
                        面试时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="engageInterview.registeTime" onclick="aa('engageInterview.registeTime')"
                               class="INPUT_STYLE2" id="date">


                    </td>
                    <td class="TD_STYLE1">
                        &nbsp;
                    </td>
                    <td class="TD_STYLE2">
                        &nbsp;
                    </td>
                    <td class="TD_STYLE1">
                        &nbsp;
                    </td>
                    <td class="TD_STYLE2">
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        面试评价
                    </td>
                    <td class="TD_STYLE2" colspan="7">
                        <textarea name="engageInterview.interviewComment" class="TEXTAREA_STYLE1" rows="4"></textarea>
                    </td>
                </tr>
            </table>
        </form>
 
    </div>
</body>
</html>
