@model Model.Major_changeModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>detail</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/vue-2.4.0.js"></script>
</head>
<body>
@using (Html.BeginForm("up", "Mobilization_audit", FormMethod.Post))
{
    <table width="100%">
        <tr>
            <td>
                <font color="black">您正在做的业务是:人力资源--培训管理--调动登记查看</font>
            </td>
        </tr>
        <tr>
            <td align="right">
                <input type="button" class="BUTTON_STYLE1" onclick="javascript:window.history.back();" value="返回">
            </td>
        </tr>
    </table>
    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr height="21">
            <td class="TD_STYLE1" width="8%">
                档案编号
            </td>
            <td class="TD_STYLE2" width="10%">
                @Html.TextBox("123", @Model.Human_id, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1" width="8%">
                姓名
            </td>
            <td class="TD_STYLE2" width="10%">
                @Html.TextBox("123", @Model.Human_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
        </tr>
        <tr>

            <td class="TD_STYLE1" width="8%">
                原一级机构
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.First_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                <input type="hidden" name="majorChange.firstKindId" value="1353318953319">
            </td>
            <td class="TD_STYLE1" width="8%">
                原二级机构
            </td>
            <td class="TD_STYLE2" width="10%">
                @Html.TextBox("123", @Model.Second_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                <input type="hidden" name="majorChange.secondKindId" value="1353318979558">
            </td>
            <td class="TD_STYLE1" width="8%">
                原三级机构
            </td>
            <td class="TD_STYLE2" width="10%">
                @Html.TextBox("123", @Model.Third_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                <input type="hidden" name="majorChange.thirdKindId" value="1353319077370">
            </td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                原职位分类
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Major_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                <input type="hidden" name="majorChange.majorKindId" value="02">
            </td>
            <td class="TD_STYLE1" width="12%">
                原职位名称
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Major_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                <input type="hidden" name="majorChange.majorId" value="02">
            </td>
            <td class="TD_STYLE1">
                原薪酬标准
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Salary_standard_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
                <input type="hidden" name="majorChange.salaryStandardId" value="1353320082662">
            </td>
            <td class="TD_STYLE1">
                原薪酬总额
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Salary_sum, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                新一级机构名称
            </td>
            <td width="15%" class="TD_STYLE2">
                <input type="hidden" name="New_first_kind_name" v-model="MFName" />
                @Html.TextBox("123", @Model.New_first_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1" width="12%">
                新二级机构名称
            </td>
            <td width="15%" class="TD_STYLE2">
                <input type="hidden" name="New_second_kind_name" v-model="MSName" />
                @Html.TextBox("123", @Model.New_second_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1" width="12%">
                新三级机构名称
            </td>
            <td width="15%" class="TD_STYLE2">
                <input type="hidden" name="New_third_kind_name" v-model="MTName" />
                @Html.TextBox("123", @Model.New_third_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1" width="8%"></td>
            <td class="TD_STYLE2" width="10%"></td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                新职位分类
            </td>
            <td class="TD_STYLE2">
                <input type="hidden" name="New_major_kind_name" v-model="MKName" />
                @Html.TextBox("123", @Model.New_major_kind_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1" width="12%">
                新职位名称
            </td>
            <td class="TD_STYLE2">
                <input type="hidden" name="New_major_name" v-model="MName" />
                @Html.TextBox("123", @Model.New_major_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1">
                新薪酬标准
            </td>
            <td class="TD_STYLE2">
                <input type="hidden" name="New_salary_standard_name" value="aa" />
                @Html.TextBox("123", @Model.New_salary_standard_name, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1">
                新薪酬总额
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.New_salary_sum, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                登记人
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Register, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1">
                登记时间
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Regist_time, new { @class = "INPUT_STYLE2", @readonly = "readonly" })

            </td>
            <td class="TD_STYLE1">
                复核人
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Checker, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
            <td class="TD_STYLE1">
                复核时间
            </td>
            <td class="TD_STYLE2">
                @Html.TextBox("123", @Model.Check_time, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                调动原因
            </td>
            <td colspan="7" class="TD_STYLE2">
                @Html.TextBox("123", @Model.Change_reason, new { @class = "INPUT_STYLE2", @readonly = "readonly" })
            </td>
        </tr>
    </table>
}
</body>
</html>
